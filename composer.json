{
    "name": "shreifelagamy/laravel-services",
    "description": "Generate Service Module",
    "type": "library",
    "require": {
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ShreifElagamy\\LaravelServices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShreifElagamy\\LaravelServices\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\ShreifElagamy\\LaravelServices\\Providers\\LaravelServicesProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Shreif Ashraf",
            "email": "shre.if.ashr.af@hotmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:l10": "composer update laravel/framework:^10.0 orchestra/testbench:^8.0 --with-all-dependencies && vendor/bin/phpunit",
        "test:l11": "composer update laravel/framework:^11.0 orchestra/testbench:^9.0 --with-all-dependencies && vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
