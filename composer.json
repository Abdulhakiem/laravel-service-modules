{
    "name": "shreifelagamy/laravel-service-modules",
    "description": "Generate modular service architecture for Laravel applications",
    "type": "library",
    "require": {
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-data": "^4.10"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ShreifElagamy\\LaravelServiceModules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShreifElagamy\\LaravelServiceModules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\ShreifElagamy\\LaravelServiceModules\\Providers\\LaravelServiceModulesProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Shreif Ashraf",
            "email": "theshreif@gmail.com",
            "homepage": "https://theshreif.dev"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:l10": "composer update laravel/framework:^10.0 orchestra/testbench:^8.0 --with-all-dependencies && vendor/bin/phpunit",
        "test:l11": "composer update laravel/framework:^11.0 orchestra/testbench:^9.0 --with-all-dependencies && vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
